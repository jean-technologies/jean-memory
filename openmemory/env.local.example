# OpenMemory Local Development Environment
# Copy this file to .env.local and update with your local Supabase keys
# 
# IMPORTANT: This file should NOT be committed to Git
# Use the values output by 'npx supabase start'

# =============================================================================
# SUPABASE LOCAL CONFIGURATION
# =============================================================================
# These values come from 'npx supabase start' output
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-local-anon-key-here
SUPABASE_SERVICE_KEY=your-local-service-role-key-here

# Local Supabase Dashboard: http://localhost:54323

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# Local Supabase PostgreSQL (automatically managed by Supabase CLI)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# =============================================================================
# VECTOR DATABASE CONFIGURATION
# =============================================================================
# Qdrant (still runs in Docker as Supabase doesn't provide vector storage)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
MAIN_QDRANT_COLLECTION_NAME=openmemory_dev

# =============================================================================
# REQUIRED API KEYS
# =============================================================================
# OpenAI Configuration (REQUIRED)
OPENAI_API_KEY=your_openai_api_key_here
LLM_PROVIDER=openai
OPENAI_MODEL=gpt-4o-mini
EMBEDDER_PROVIDER=openai
EMBEDDER_MODEL=text-embedding-3-small

# =============================================================================
# OPTIONAL INTEGRATIONS
# =============================================================================
# Gemini (Optional)
GEMINI_API_KEY=

# Anthropic (Optional)
ANTHROPIC_API_KEY=

# Apify for Twitter scraping (Optional)
APIFY_TOKEN=

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================
DEBUG=true
LOG_LEVEL=INFO
PYTHONUNBUFFERED=1

# Next.js settings
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=development 